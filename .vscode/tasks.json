{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"command": "cargo",
			"args": ["run", "--example", "example"],
			"problemMatcher": ["$rustc"],
			"label": "rust: cargo run",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"echo": true,
				"reveal": "always",
				"focus": false,
				"panel": "shared",
				"showReuseMessage": true,
				"clear": true,
				"close": false
			}
		}
	]
}
